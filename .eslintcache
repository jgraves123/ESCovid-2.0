Nav.Link>\n          </Nav>\n        </Navbar.Collapse>\n      </Navbar>\n    </div>\n  );\n}\n\nexport default withRouter(Navigation);\n","/Users/John/Projects/escovid2/src/components/site/Contact.jsx",["155"],"import React from \"react\";\nimport GliesePopUp from \"../escovid/GliesePopUp\";\n\nfunction Contact() {\n  return (\n    <div className=\"contact\">\n      <div class=\"container\">\n        <div class=\"row align-items-center my-5\">\n          <div class=\"col-lg-7\">\n            <img\n              class=\"img-fluid rounded mb-4 mb-lg-0\"\n              src=\"https://raw.githubusercontent.com/jgraves123/ESCovid-2.0/master/images/gaudi.JPG?raw=true\"\n              alt=\"\"\n            />\n          </div>\n          <div class=\"col-lg-5\">\n            <h1 class=\"font-weight-light\">Contact Us</h1>\n            <p>\n              Email us at mlagerq@emory.edu and john_graves@brown.edu. We are happy to answer any questions!\n            </p>\n            <br/>\n            <p>Tried one of our escape games? Fill out the feedback form here:</p><a href={\"https://docs.google.com/forms/d/e/1FAIpQLSdRNn2uICAC4WlS4blWU4G07OpPJJU_M5NPN17GlNVfWP7Z2A/viewform\"}>ESCovid</a>\n          </div>\n        </div>\n      </div>\n    </div>\n  );\n}\n\nexport default Contact;\n","/Users/John/Projects/escovid2/src/components/site/About.jsx",[],"/Users/John/Projects/escovid2/src/components/site/Home.jsx",["156"],"import React from \"react\";\nimport Leader from \"../game/leaderboard\";\nimport {Carousel, Navbar, Image, Container, Row, Col} from 'react-bootstrap';\n\nfunction Home() {\n  localStorage.clear()\n  return (\n    <div>\n          <div align={\"center\"} style={{marginTop: \"20px\"}}>\n            <div style={{width: \"90%\"}}>\n            <Carousel interval={50000} pause={\"false\"} style={{height: \"100%\"}}>\n              <Carousel.Item align='center' style={{backgroundColor: \"#fcc81a\"}}>\n                <div className=\"sideContent\" style={{backgroundColor: \"#fcc81a\"}}>\n                  <div className=\"sideText\">\n                    <h1>Welcome</h1>\n                    <br/>\n                    <p>You can check in anytime you like, <br/>\n                      but you can never leave...</p>\n                  </div>\n                </div>\n                <div style={{width: \"70%\", float: \"left\"}}>\n                  <img\n                      className=\"d-block w-100\"\n                      src=\"https://raw.githubusercontent.com/jgraves123/ESCovid-2.0/master/images/viper12.png?raw=true\"\n                      alt=\"First slide\"\n                  />\n                </div>\n              </Carousel.Item>\n              <Carousel.Item align='center' style={{backgroundColor: \"#0fd145\"}}>\n                <div className=\"sideContent\" style={{backgroundColor: \"#0fd145\"}}>\n                  <div className=\"sideText\">\n                    <h1>Play now!</h1>\n                    <br/>\n                    <p>Space + math + world crisis. <br/>What more do you need?</p>\n                  </div>\n                </div>\n                <div style={{width: \"70%\", float: \"left\"}}>\n                  <img\n                      className=\"d-block w-100\"\n                      src=\"https://raw.githubusercontent.com/jgraves123/ESCovid-2.0/master/images/escovid12.png?raw=true\"\n                      alt=\"escovid\"\n                  />\n                </div>\n              </Carousel.Item>\n              <Carousel.Item style={{backgroundColor: \"#fcc81a\"}}>\n                <div className=\"sideContent\" style={{backgroundColor: \"#fcc81a\"}}>\n                  <div className=\"sideText\">\n                    <h1>Coming Soon!</h1>\n                    <br/>\n                    <p>Wouldn't it be funny if were actually trapped in here?</p>\n                  </div>\n                </div>\n                <div style={{width: \"70%\", float: \"left\"}}>\n                  <img\n      